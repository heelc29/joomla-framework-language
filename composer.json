{
    "name": "joomla/language",
    "type": "joomla-package",
    "description": "Joomla Language Package",
    "keywords": ["joomla", "framework", "language"],
    "homepage": "https://github.com/joomla-framework/language",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~8.1.0",
        "ext-xml": "*",
        "joomla/string": "^2.0",
        "symfony/deprecation-contracts": "^2.1"
    },
    "require-dev": {
        "joomla/di": "^2.0",
        "joomla/registry": "^2.0",
        "joomla/test": "^2.0",
        "phpunit/phpunit": "^8.5|^9.0",
        "squizlabs/php_codesniffer": "~3.7.1"
    },
    "suggest": {
        "joomla/di": "To use the Language ServiceProviderInterface objects, install joomla/di."
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Language\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Language\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev",
            "dev-3.x-dev": "3.0-dev"
        }
    }
}
